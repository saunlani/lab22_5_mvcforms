@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>

    <form name="daform" action="/Home/Login" method="get">
        
        Username:<br /><input type="text" name="_username" /><br />
        
        <br />Select a color:<br />
        <select name="_color">
        <option value="Red">Red</option>
        <option value="Yellow">Yellow</option>
        <option value="BLue">Blue</option>
        </select><br />
        
        What are you listening to right now?:<br /><input type="text" name="_listen" /><br />

        <input type="submit" name="Register!" />
        <input type="reset" name="Reset" /><br />
    </form>
    <p class="lead">
    </p>
</div>

